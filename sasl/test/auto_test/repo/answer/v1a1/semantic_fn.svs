; ModuleID = 'prog'

%.s.bufi = type <{ float }>
%.s.bufo = type <{ float }>
%.s.stri = type <{ float* }>
%.s.stro = type <{}>

define void @fn(%.s.stri* %.arg.stri, %.s.bufi* %.arg.bufi, %.s.stro* %.arg.stro, %.s.bufi* %.arg.bufo) {
.init.vargs:

.entry:                                           ; preds = %3
  %0 = load %.s.bufi* %.arg.bufi
  %1 = extractvalue %.s.bufi %0, 0
  %2 = getelementptr %.s.bufi* %.arg.bufo, i32 0, i32 0
  store float %1, float* %2
  ret void
                                                  ; No predecessors!
  br label %.entry
}
